{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
					"indent": [ "error", 3 ],
					"quotes": ["error", "single", { "avoidEscape": true }],
					"semi": ["error", "always"]
  }
}
